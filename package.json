{
	"name": "worker",
	"version": "0.0.0",
	"description": "cloudflare worker",
	"main": "./dist/main.js",
	"scripts": {
		"dev": "concurrently \"yarn:js:dev\" \"yarn:wrgl:dev\" -k --restart-tries 5 --restart-after 1000",
		"js:dev": "nodemon",
		"js:build": "node ./scripts/build.js --minify",
		"wrgl": "wrangler",
		"wrgl:pub": "yarn js:build && wrangler publish",
		"wrgl:dev": "wrangler dev"
	},
	"author": "Jan Schmidt <me@jnschmdt.de>",
	"license": "MIT",
	"devDependencies": {
		"@cloudflare/workers-types": "^2.2.2",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"concurrently": "^6.1.0",
		"esbuild": "^0.11.17",
		"eslint": "^7.25.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.4.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-prettier": "^3.4.0",
		"nodemon": "^2.0.7",
		"prettier": "^2.2.1",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"bowser": "^2.11.0"
	}
}

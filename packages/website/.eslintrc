{
	"extends": ["../../.eslintrc"],
	"env": {
		"browser": true,
		"es6": true,
		"jest": true,
		"node": true
	},
	"overrides": [
		{
			"files": ["**/*.svelte"],
			"processor": "svelte3/svelte3"
		}
	],
	"parserOptions": {
		"ecmaVersion": 2019,
		"sourceType": "module"
	},
	"plugins": ["import", "svelte3"]
}
